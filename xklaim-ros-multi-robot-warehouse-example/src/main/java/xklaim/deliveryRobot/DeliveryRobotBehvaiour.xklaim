package xklaim.arm

import klava.Locality
import xklaim.arm.DeliveryOneItem
import static xklaim.deliveryRobot.DeliveryRobotConstants.*

proc DeliveryRobotBehaviour(String robotId,String sector,Locality Arm) {
	eval(new DeliveryOneItem(robotId,sector,Arm))@self
	in(AVAILABLE_FOR_DELIVERY)@self
	eval(new DeliveryRobotBehaviour(robotId,sector,Arm))@self
}
