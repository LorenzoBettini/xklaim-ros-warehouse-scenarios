package xklaim.arm

import klava.Locality
import xklaim.arm.DeliveryOneItem

proc DeliveryRobotBehaviour(String rosbridgeWebsocketURI,String robotId,String sector,Locality Arm) {
	eval(new DeliveryOneItem(rosbridgeWebsocketURI,robotId,sector,Arm))@self
	
	in("availableForDelivery")@self
	
	eval(new DeliveryRobotBehaviour(rosbridgeWebsocketURI,robotId,sector,Arm))@self
}
